#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/c/Users/tfunk/bolt/project-bolt-github-nixuwvay (1)/project/node_modules/.pnpm/react-native@0.77.0_@babel+core@7.26.0_@babel+preset-env@7.26.0_@babel+core@7.26.0__@react-na_f4fyte6otifsdbge4mbjal2zh4/node_modules/react-native/node_modules:/mnt/c/Users/tfunk/bolt/project-bolt-github-nixuwvay (1)/project/node_modules/.pnpm/react-native@0.77.0_@babel+core@7.26.0_@babel+preset-env@7.26.0_@babel+core@7.26.0__@react-na_f4fyte6otifsdbge4mbjal2zh4/node_modules:/mnt/c/Users/tfunk/bolt/project-bolt-github-nixuwvay (1)/project/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/c/Users/tfunk/bolt/project-bolt-github-nixuwvay (1)/project/node_modules/.pnpm/react-native@0.77.0_@babel+core@7.26.0_@babel+preset-env@7.26.0_@babel+core@7.26.0__@react-na_f4fyte6otifsdbge4mbjal2zh4/node_modules/react-native/node_modules:/mnt/c/Users/tfunk/bolt/project-bolt-github-nixuwvay (1)/project/node_modules/.pnpm/react-native@0.77.0_@babel+core@7.26.0_@babel+preset-env@7.26.0_@babel+core@7.26.0__@react-na_f4fyte6otifsdbge4mbjal2zh4/node_modules:/mnt/c/Users/tfunk/bolt/project-bolt-github-nixuwvay (1)/project/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../react-native/cli.js" "$@"
else
  exec node  "$basedir/../react-native/cli.js" "$@"
fi
