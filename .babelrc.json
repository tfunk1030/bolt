{
    "presets": [
      ["babel-preset-expo", {
        "jsxImportSource": "nativewind",
        "jsxRuntime": "automatic"
      }]
    ],
    "plugins": [
      "nativewind/babel",
      ["module-resolver", {
        "root": ["./src"],
        "alias": {
          "@core": "./src/core",
          "@features": "./src/features",
          "@navigation": "./src/navigation"
        }
      }]
    ]
  }
  